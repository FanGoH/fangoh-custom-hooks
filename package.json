{
	"name": "fangoh-custom-hooks",
	"version": "0.0.1",
	"description": "React re-usable Custom Hooks",
	"main": "dist/index.ts",
	"scripts": {
		"build": "rollup -c",
		"start": "rollup -c -w",
		"test": "jest"
	},
	"keywords": [
		"react",
		"hooks",
		"custom-hooks"
	],
	"jest": {
		"transform": {
			".(ts|tsx)": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"modulePathIgnorePatterns": [
			"<rootDir>/dist/"
		]
	},
	"files": [
		"dist"
	],
	"author": "Hector Osuna",
	"license": "ISC",
	"dependencies": {
		"react": "^17.0.2"
	},
	"devDependencies": {
		"@testing-library/react-hooks": "^5.1.0",
		"@types/jest": "^26.0.22",
		"@types/react": "^17.0.3",
		"babel": "^6.23.0",
		"babel-core": "^6.26.3",
		"babel-runtime": "^6.26.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"jest": "^26.6.3",
		"react-test-renderer": "^17.0.2",
		"rollup": "^2.42.4",
		"rollup-plugin-typescript2": "^0.30.0",
		"rollup-plugin-uglify": "^6.0.4",
		"ts-jest": "^26.5.4",
		"typescript": "^4.2.3"
	},
	"typings": "./dist/index.d.ts"
}
